{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/js/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/dist/bootstrap/",
      "files": [ "dist/js/bootstrap.min.js", "dist/css/bootstrap.min.css" ]
    },
    {
      "provider": "unpkg",
      "library": "popper.js@1.16.0",
      "destination": "wwwroot/js/popper/"
    },
    {
      "provider": "unpkg",
      "library": "svg-pan-zoom@3.6.1",
      "destination": "wwwroot/js/svgpanzoom/"
    }
  ]
}